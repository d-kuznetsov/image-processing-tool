{
  "name": "image-uploader",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "docker-build:dev": "docker build -t img-manager-dev -f Dockerfile.dev .",
    "docker-build:prod": "docker build -t img-manager-prod -f Dockerfile.prod .",
    "docker-run:dev": "docker run -d -p 3000:3000 --name img-manager-dev -v upload-vol-dev:/app/upload -v src-vol-dev:/app/src img-manager-dev",
    "docker-run:prod": "docker run -d -p 3000:3000 --name img-manager-prod -v upload-vol-prod:/app/upload img-manager-prod",
    "docker-stop:dev": "docker stop img-manager-dev",
    "docker-stop:prod": "docker stop img-manager-prod",
    "docker-start:dev": "docker start img-manager-dev",
    "docker-start:prod": "docker start img-manager-prod"
  },
  "dependencies": {
    "@types/lowdb": "^1.0.9",
    "axios": "^0.19.2",
    "get-image-colors": "^2.0.1",
    "lowdb": "^1.0.0",
    "multer": "^1.4.2",
    "next": "9.3.6",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "serve-handler": "^6.1.3",
    "sharp": "^0.25.4",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^1.8.10"
  }
}
